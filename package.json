{
  "name": "os-tech",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json --disable-host-check",
    "start:develop": "ng serve --proxy-config proxy.conf.develop.json --configuration develop",
    "start:staging": "ng serve --proxy-config proxy.conf.staging.json --configuration staging",
    "start:testing": "ng serve --proxy-config proxy.conf.testing.json --configuration testing",
    "build": "ng build",
    "build:prod": "ng build --configuration=production",
    "build:optimized": "ng build --configuration=${ENV} --vendor-chunk --common-chunk",
    "build:optimized:stats": "ng build --configuration=${ENV} --aot --build-optimizer --optimization --vendor-chunk --common-chunk --stats-json",
    "test": "ng test",
    "test:karma": "ng test --karma-config=karma.config.js --code-coverage",
    "lint": "ng lint os-tech",
    "lint:fix": "ng lint os-tech --fix",
    "pretty": "prettier --config ./.prettierrc --parser typescript -l",
    "format:check": "prettier --config ./.prettierrc --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss}\"",
    "format:fix:all": "pretty-quick",
    "format:fix:staged": "pretty-quick --staged",
    "commitmsg": "commitlint -E GIT_PARAMS"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@popperjs/core": "^2.11.8",
    "apexcharts": "^3.42.0",
    "bootstrap": "^5.3.0",
    "firebase": "^10.12.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.8",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "~15.2.8",
    "@angular/compiler-cli": "^15.2.0",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^20.2.5",
    "@typescript-eslint/eslint-plugin": "5.48.2",
    "@typescript-eslint/parser": "5.48.2",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^46.2.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "^8.0.3",
    "istanbul": "^0.4.5",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "karma-sonarqube-reporter": "^1.4.0",
    "moment-timezone": "^0.5.43",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "sonar-scanner": "^3.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~4.9.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s format:fix:staged lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
